<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Lista Członków</title>
  <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
  <div class="container">
    <h1 class="title">Lista Członków</h1>
    <table class="table">
      <thead>
        <tr>
          <th>Index</th>
          <th>First Name</th>
          <th>Last Name</th>
        </tr>
      </thead>
      <tbody>
        @each(member in members)
          <tr>
            <td>{{ member.id }}</td>
            <td>{{ member.firstName }}</td>
            <td>{{ member.lastName }}</td>
          </tr>
        @endeach
      </tbody>
    </table>
  </div>
</body>
</html>